{
    "name": "debril/rss-atom-bundle",
    "type": "symfony-bundle",
    "description": "RSS / Atom and JSONFeed support for Symfony",
    "keywords": ["rss", "atom", "jsonfeed", "feed", "syndication"],
    "homepage": "http://debril.org/category/rss-atom-bundle/",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alexdebril/rss-atom-bundle.git"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/config": "^5.4|^6.4|^7.0|^8.0",
        "symfony/http-foundation": "^5.4|^6.4|^7.0|^8.0",
        "symfony/http-kernel": "^5.4|^6.4|^7.0|^8.0",
        "symfony/dependency-injection": "^5.4|^6.4|^7.0|^8.0",
        "php-feed-io/feed-io": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "doctrine/common": "^2.3|^3.0",
        "symfony/validator": "^5.4|^6.4|^7.0|^8.0",
        "symfony/finder": "^5.4|^6.4|^7.0|^8.0",
        "symfony/browser-kit": "^5.4|^6.4|^7.0|^8.0",
        "symfony/yaml": "^5.4|^6.4|^7.0|^8.0",
        "symfony/framework-bundle": "^5.4|^6.4|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Debril\\RssAtomBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Debril\\RssAtomBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.34"
        },
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        }
    }
}
